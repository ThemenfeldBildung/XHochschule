    </angabenQualifikation>
    <dauerUndEbeneQualifikation>
        <ebeneDerQualifikation xml:lang="de-DE">First Degree including thesis</ebeneDerQualifikation>
        <ebeneDerQualifikationCodeEQF listURI="urn:xbildung-de:publicationsoffice-eu:codeliste:eqf" listVersionID="20230118-0">
