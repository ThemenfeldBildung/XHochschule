 xsi:schemaLocation="http://xhochschule.de/def/xhochschule/1.1/xsd/xhochschule.xsd">
    <xhs:titel xml:lang="de-DE">Diploma Supplement</xhs:titel> 
    <xhs:ausstellungsdatum>2024-10-21</xhs:ausstellungsdatum>
    <xhs:sprache listURI="urn:xbildung-de:publicationsoffice-eu:codeliste:language" listVersionID="20240925-0">
        <code>http://publications.europa.eu/resource/authority/language/DEU</code>
    </xhs:sprache>
    <!-- Angaben zum Inhaber des Dokumentes -->
    <xhs:angabenInhaber>
        <xhs:familienname>Musterfrau</xhs:familienname>
        <xhs:vorname>Erika</xhs:vorname>
        <xhs:geburtsdatum>
            <xbd:jahrMonat>1994-09</xbd:jahrMonat>
        </xhs:geburtsdatum>
        <xhs:matrikelnummer>123456789</xhs:matrikelnummer>
    </xhs:angabenInhaber>
    <!-- Angaben zur Qualifikation -->
    <xhs:angabenQualifikation>
        <xhs:bezeichnungQualifikationGrad xml:lang="de-DE">Diplom-Kaufmann</xhs:bezeichnungQualifikationGrad>
        <xhs:hauptstudienfach xml:lang="de-DE">Business Studies</xhs:hauptstudienfach>
        <xhs:verleihendeEinrichtung xml:lang="de-DE">Technische Universität Musterstadt</xhs:verleihendeEinrichtung>
        <xhs:durchfuehrendeEinrichtung xml:lang="de-DE">Technische Universität Musterstadt</xhs:durchfuehrendeEinrichtung>
        <xhs:spracheUnterrichtPruefung listURI="urn:xbildung-de:publicationsoffice-eu:codeliste:language" listVersionID="20240925-0">
            <code>http://publications.europa.eu/resource/authority/language/DEU</code>
        </xhs:spracheUnterrichtPruefung>
    </xhs:angabenQualifikation>
    <xhs:dauerUndEbeneQualifikation>
        <xhs:ebeneDerQualifikation>
            <xhs:ebeneDerQualifikationCodeEQF listURI="urn:xbildung-de:publicationsoffice-eu:codeliste:eqf" listVersionID="20230118-0">
