        <code>DEU</code>
    </sprache>
    <!-- Angaben zum Inhaber des Dokumentes -->
    <angabenInhaber>
        <familienname>Musterfrau</familienname>
        <vorname>Erika</vorname>
        <geburtsdatum>
            <jahr>1994</jahr>
        </geburtsdatum>
        <matrikelnummer>123456789</matrikelnummer>
    </angabenInhaber>
    <!-- Angaben zur Qualifikation -->
    <angabenQualifikation>
        <bezeichnungQualifikationGrad xml:lang="de-DE">Diplom-Kaufmann</bezeichnungQualifikationGrad>
        <hauptstudienfach xml:lang="de-DE">Business Studies</hauptstudienfach>
        <verleihendeEinrichtung xml:lang="de-DE">Technische Universität Musterstadt </verleihendeEinrichtung>
        <durchfuehrendeEinrichtung xml:lang="de-DE">Technische Universität Musterstadt</durchfuehrendeEinrichtung>
