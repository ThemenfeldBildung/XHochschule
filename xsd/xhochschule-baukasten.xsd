   <xs:complexType name="Code.Leistungsbenotungsstatus">
      <xs:annotation>
         <xs:appinfo>
            <codeliste>
               <nameLang>Leistungsbenotungsstatus</nameLang>
               <nameKurz>Leistungsbenotungsstatus</nameKurz>
